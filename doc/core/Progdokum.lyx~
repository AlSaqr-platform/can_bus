\begin_layout Subsection
3.4.1 BTR
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
label{sec:BTR}
\end_layout

\end_inset


\end_layout

\begin_layout Description
Type: Read / Write
\end_layout

\begin_layout Standard
The length of bit time segments for Nominal bit time in Time quanta.
 Note that SYNC segment always lasts one Time quanta.
\begin_inset Separator latexpar
\end_inset


\end_layout

